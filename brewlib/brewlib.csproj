<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <RootNamespace>BrewLib</RootNamespace>
    <AssemblyName>BrewLib</AssemblyName>
    <TargetFramework>net481</TargetFramework>
    <Copyright>Copyright © Damnae 2016</Copyright>
    <UseWindowsForms>true</UseWindowsForms>
    <Product>brewlib</Product>
    <Title>brewlib</Title>
    <Version>1.1</Version>
    <Company>storybrew</Company>
    <Platforms>AnyCPU;x86</Platforms>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <EnableNETAnalyzers>True</EnableNETAnalyzers>
    <AnalysisLevel>latest-recommended</AnalysisLevel>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>embedded</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>embedded</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x86'">
    <DebugType>embedded</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x86'">
    <DebugType>embedded</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="oxipng.exe" />
    <EmbeddedResource Include="pngquant.exe" />
    <EmbeddedResource Include="truepng.exe" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="OpenTK" Version="3.3.3" />
    <PackageReference Include="ManagedBass" Version="3.1.1" />
    <PackageReference Include="ManagedBass.Fx" Version="3.1.1" />
  </ItemGroup>
	
  <ItemGroup>
	<ProjectReference Include="..\tiny\tiny.csproj" />
  </ItemGroup>
	
  <ItemGroup>
    <Reference Include="System.Management" />
	<Reference Include="System.Web" />
  </ItemGroup>
	
  <ItemGroup>
    <None Update="C:\Users\die lin\.nuget\packages\magick.net-q8-anycpu\13.3.0\build\netstandard20\..\..\runtimes\win-arm64\native\Magick.Native-Q8-arm64.dll">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>